<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        xmlns:views="clr-namespace:GUI.View"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="GUI.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">


    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="46*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="65"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="12.218"/>
        </Grid.RowDefinitions>
        <Button x:Name="button" Content="Connect" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" Height="20"/>
        <Button x:Name="button1" Content="Run" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="442,35,0,0" Click="button1_Click" Height="20" Grid.ColumnSpan="2"/>
        <ComboBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="0,35,0,0" Text="" VerticalAlignment="Top" Width="427" IsReadOnly="False" IsEditable="True">
            <System:String>{"Command":"LineStatus","LineStatusLogin":"linelogin"}</System:String>
            <System:String>{"Command":"GetSipLogs","LineSipLogLogin":"linelogin"}</System:String>
        </ComboBox>
        <Button x:Name="button2" Content="Clear" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="75" Click="button2_Click"/>
        <RichTextBox x:Name="richTextBox" Grid.Row="1" Grid.ColumnSpan="2">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
    </Grid>
</Window>
